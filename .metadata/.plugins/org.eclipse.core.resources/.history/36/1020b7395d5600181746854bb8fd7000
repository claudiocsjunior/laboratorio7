Movimentacao::Movimentacao() {
}

Movimentacao::Movimentacao(std::string descricao, double valor, int tipo) {
}

Movimentacao::~Movimentacao() {
}

const std::string& Movimentacao::getDescricao() const {
	return descricao;
}

void Movimentacao::setDescricao(const std::string& descricao) {
	this->descricao = descricao;
}

int Movimentacao::getTipo() const {
	return tipo;
}

void Movimentacao::setTipo(int tipo) {
	this->tipo = tipo;
}

double Movimentacao::getValor() const {
	return valor;
}

void Movimentacao::setValor(double valor) {
	this->valor = valor;
}
