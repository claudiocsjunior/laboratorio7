#include <vector>

class Agencia
{
private:
	int quantidade_contas;
	std::vector<ContaCorrente> conta_corrente;
public:
	Agencia();
	~Agencia();
	bool adicionar_conta(ContaCorrente &conta_corrente);
	bool excluir_conta(ContaCorrente &conta_corrente);

};

#endif
